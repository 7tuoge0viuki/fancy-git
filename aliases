alias fancygit="sh ~/.fancy-git/commands.sh $1"
alias gs="clear && git status"
alias ga="git add"
alias gaa="git add --all"
alias gd="clear && git diff"
alias gcm="git commit"
alias gck="git checkout --"
alias gcb="git checkout -b"
alias gdb="git branch -D"
alias gdbr="git push origin --delete"
alias gl="git log"
alias glp="git log --pretty=oneline"
alias gdof="find . -name "*.orig" | xargs rm -rf"
alias gfi="git update-index --assume-unchanged"
alias gurl="sh ~/.fancy-git/alias_functions/gurl.sh"
